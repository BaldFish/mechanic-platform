<!--pages/carType/carType.wxml-->
<view class="carSeries" hover-class="none" hover-stop-propagation="false">
  <view class="list" hover-class="none" hover-stop-propagation="false" wx:for="{{carYears}}" wx:key="index" bindtap="turnDetails" data-value="{{item}}">
    <view class="left_text" hover-class="none" hover-stop-propagation="false">{{item}}</view>
    <image class="right_img" src="/images/jiantou.png"></image>
  </view>
</view>
