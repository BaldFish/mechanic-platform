/* pages/partner/partner.wxss */
.partner{
  padding-top: 38px;
}
.operation-wrap{
  width: 700rpx;
  padding: 40rpx 0 60rpx 90rpx;
  background-color: #ffffff;
  box-shadow: 0px 0px 53px 9px rgba(94, 123, 246, 0.1);
  border-radius: 30rpx;
  box-sizing: border-box;
  margin: 0 auto;
}
.operation-wrap text{
  display: block;
}
.operation-title{
  font-size: 18px;
  color: #386cff;
}
.explain-list{
  font-size: 12px;
  color: #222222;
}
.operation-list{
  font-size: 11px;
  color: #333333;
  height: 18px;
  line-height: 18px;
}
.submitPhone_wrap{
  width: 700rpx;
  height: 400rpx;
  background-color: #ffffff;
  box-shadow: 0px 0px 53px 9px rgba(94, 123, 246, 0.1);
  border-radius: 30rpx;
  box-sizing: border-box;
  margin: 0 auto;
  margin-top: 28px;
}